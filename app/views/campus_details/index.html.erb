<h1>Listing campus_details</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @campus_details.each do |campus_detail| %>
  <tr>
    <td><%= campus_detail.name %></td>
    <td><%= campus_detail.address %></td>
    <td><%= link_to 'Show', campus_detail %></td>
    <td><%= link_to 'Edit', edit_campus_detail_path(campus_detail) %></td>
    <td><%= link_to 'Destroy', campus_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

