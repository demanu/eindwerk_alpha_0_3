<%= semantic_form_for @book_edition do |f| %>
  <%= f.inputs do %>
    <%= f.input :bookwork %>
    <%= f.input :isbn_nr %>
    <%= f.input :book_format, :member_label => :format %>
    <%= f.input :publisher %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
