<h1>Listing book_formats</h1>

<table>
  <tr>
    <th>Format</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @book_formats.each do |book_format| %>
  <tr>
    <td><%= book_format.format %></td>
    <td><%= link_to 'Show', book_format %></td>
    <td><%= link_to 'Edit', edit_book_format_path(book_format) %></td>
    <td><%= link_to 'Destroy', book_format, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

